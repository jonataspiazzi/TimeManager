{
  "name": "time-manager",
  "version": "0.1.0",
  "private": true,
  "main": "src-main/index.ts",
  "homepage": "./",
  "scripts": {
    "render.dev": "react-app-rewired start",
    "render.dev.bn": "cross-env BROWSER=none react-app-rewired start",
    "render.build": "react-app-rewired build",
    "render.test": "react-app-rewired test",
    "main.tsc": "tsc --watch --project ./src-main/tsconfig.json",
    "main.exec": "cross-env NODE_ENV=dev nodemon --exec \"\"electron ./dist-main/index.js\"\"",
    "main.dev": "concurrently \"npm run main.tsc\" \"npm run main.exec\"",
    "dev": "concurrently \"npm run render.dev.bn\" \"npm run main.dev\""
  },
  "build": {
    "appId": "time.manager",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    },
    "mac": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.4",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "electron-is-dev": "^1.2.0",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.6",
    "react-app-rewired": "^2.1.6"
  }
}
